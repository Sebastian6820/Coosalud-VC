SELECT
SUBSTRING(CORTE_BDUA ,1,4) Ano,
SUBSTRING(CORTE_BDUA ,6,2) Mes,
CODIGO_DE_LA_ENTIDAD,
FECHA_DE_NACIMIENTO_DEL_AFILIADO,
SEXO_DEL_AFILIADO,
DIVIPOLA,
ZONA,
FECHA_DE_LA_AFILIACION_Y_O_NOVEDAD,
EDAD,
TIPO_INGRESO,
TIPO_ARCHIVO,
CODIGO_DE_LA_ENTIDAD_DESTINO,
TOTAL_AFILIADOS
FROM DW_CICLO_INGRESO_UPC.VAULT_FUENTES.V_AFILIADOS_CEDIDOS_GEN 
WHERE CORTE_BDUA > '2018-12-31'
